#------------------------------------ Apps --------------------------------------
#--------------------------------------------------------------------------------
"pcmanfm"
	Control + Alt + f

"rofi -show run"
	Alt + F2

"alacritty"
	Control + Alt + t

"alacritty -e termdown"
	Control + Alt + s



#--------------------------------------------------------------------------------
#------------------------------------ Media -------------------------------------
#--------------------------------------------------------------------------------
"media.sh show"
	Control + F9

"media.sh toggle"
	Control + F10

"media.sh next"
	Control + F12

"media.sh prev"
	Control + F11

"recscr.sh"
	Control + Alt + Shift + R



#--------------------------------------------------------------------------------
#------------------------------- Screenshots ------------------------------------
#--------------------------------------------------------------------------------
"screenshot.sh all file"
	Print

"screenshot.sh window file"
	Alt + Print

"screenshot.sh area file"
	Shift + Print

"screenshot.sh all clipboard"
	Control + Print

"screenshot.sh window clipboard"
	Control + Alt + Print

"screenshot.sh area clipboard"
	Control + Shift + Print



#--------------------------------------------------------------------------------
#------------------------------------ Volume ------------------------------------
#--------------------------------------------------------------------------------
"volume.sh mute"
	XF86AudioMute

"volume.sh up"
	XF86AudioRaiseVolume

"volume.sh down"
	XF86AudioLowerVolume

